{
    "name": "CopyTopologyDataPipeline",
    "properties": {
        "description": "Copy Topology reference data to Sql Azure",
        "activities": [
            {
                "type": "Copy",
                "typeProperties": {
                    "source": {
                        "type": "BlobSource",
                        "treatEmptyAsNull": true,
                        "skipHeaderLineCount": 1
                    },
                    "sink": {
                        "type": "SqlSink",
                        "writeBatchSize": 0,
                        "writeBatchTimeout": "01:00:00"
                    }
                },
                "inputs": [
                    {
                        "name": "TopologyRefDataTable"
                    }
                ],
                "outputs": [
                    {
                        "name": "SQLTopologyRefDataTable"
                    }
                ],
                "policy": {
                    "timeout": "10:00:00",
                    "concurrency": 1,
                    "retry": 3
                },
                "scheduler": {
                    "frequency": "Day",
                    "interval": 1,
                    "style": "StartOfInterval"
                },
                "name": "CopyTopologyReftoSqlAzure",
                "description": "Copy topology reference data table to Sql Azure"
            }
        ],
        "start": "<The start date-time of the duration in which data processing will occur or the data slices will be processed. Example : 2014-05-01T00:00:00Z>",
        "end": "<The end date-time of the duration in which data processing will occur or the data slices will be processed. Example: 2014-05-05T00:00:00Z>",
        "isPaused": false
    }
}